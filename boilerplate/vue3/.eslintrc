{
  "extends": "eslint",
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "no-tabs": "off",
    "new-cap": "off",
    "newline-after-var": "off",
    "no-invalid-this": "off",
    "quotes": [
      "error",
      "single"
    ],
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "guard-for-in": "off",
    "no-console": "off",
    "no-undefined": "off",
    "array-bracket-spacing": "off",
    "no-unused-expressions": "off",
    "func-style": [
      "error",
      "expression"
    ],
    "comma-dangle": [
      "error",
      "never"
    ]
  },
  "globals": {
    "window": true
  },
  "parser": "babel-eslint"
}
