  "files": [
    "config",
    "src",
    "test",
    "tsconfig.json",
    "babel.config.js",
    "jest.config.js",
    ".eslintrc.js",
    ".eslintignore",
    ".gitignore",
    ".fatherrc.js",
    "yarn.lock"
  ]
