{
  "name": "easy-component",
  "version": "1.0.0",
  "description": "Building React Component By Webpack",
  "main": "dist/easy.js",
  "module": "src/index.js",
  "scripts": {
    "clean": "easy clean",
    "dev": "easy dev",
    "start": "concurrently \"npm run dev\" \"npm run server\"",
    "debug": "easy build test",
    "build": "easy build",
    "server": "easy server -p 5000 -i example/index.html"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.9.4",
    "@easy-team/easywebpack-cli": "^4.0.0",
    "@easy-team/easywebpack-js": "4.0.0",
    "eslint": "^4.5.0",
    "eslint-config-eslint": "^4.0.0"
  },
  "keywords": [
    "webpack lib",
    "typescript lib"
  ],
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "test/*.test.js"
    ]
  },
  "ci": {
    "version": "8, 10, 12"
  },
  "repository": {
    "type": "git",
    "url": "git+http://github.com/easy-team/easywebpack-awesome.git"
  },
  "homepage": "http://github.com/easy-team/easywebpack-awesome#readme",
  "author": "hubcarl@126.com"
}
